{
  "name": "@terminus/ngx-tools",
  "version": "0.0.1",
  "description": "A collection of tools and utilities for the Terminus (Angular/NGRX) applications",
  "keywords": [
    "angular",
    "webpack",
    "typescript"
  ],
  "author": "@terminus",
  "homepage": "https://github.com/GetTerminus/ngx-tools",
  "license": "MIT",
  "scripts": {
    "build:aot:prod": "yarn run clean:dist && yarn run clean:aot && cross-env BUILD_AOT=1 yarn run webpack --config config/webpack.prod.js  --progress --profile --bail",
    "build:aot": "yarn run build:aot:prod",
    "build:dev": "yarn run clean:dist && yarn run webpack --config config/webpack.dev.js --progress --profile",
    "build:docker": "yarn run build:prod && docker build -t angular2-webpack-start:latest .",
    "build:prod": "yarn run clean:dist && yarn run webpack --config config/webpack.prod.js  --progress --profile --bail",
    "build": "yarn run build:dev",
    "ci:aot": "yarn run lint && yarn run test && yarn run build:aot && yarn run e2e",
    "ci:jit": "yarn run lint && yarn run test && yarn run build:prod && yarn run e2e",
    "ci:nobuild": "yarn run lint && yarn test && yarn run e2e",
    "ci:testall": "yarn run lint && yarn run test && yarn run build:prod && yarn run e2e && yarn run build:aot && yarn run e2e",
    "ci:travis": "yarn run lint && yarn run test && yarn run build:aot && yarn run e2e:travis",
    "ci": "yarn run ci:testall",
    "clean:dll": "yarn run rimraf dll",
    "clean:aot": "yarn run rimraf compiled",
    "clean:dist": "yarn run rimraf dist",
    "clean:install": "yarn set progress=false && yarn install",
    "clean": "yarn cache clean --force && yarn run rimraf node_modules doc coverage dist compiled dll",
    "docker": "docker",
    "docs": "yarn run docs:toc",
    "e2e:live": "npm-run-all -p -r server:prod:ci protractor:live",
    "e2e:travis": "npm-run-all -p -r server:prod:ci protractor:delay",
    "e2e": "npm-run-all -p -r server:prod:ci protractor",
    "github-deploy:dev": "yarn run webpack --config config/webpack.github-deploy.js --progress --profile --env.githubDev",
    "github-deploy:prod": "yarn run webpack --config config/webpack.github-deploy.js --progress --profile --env.githubProd",
    "github-deploy": "yarn run github-deploy:dev",
    "gulp": "gulp",
    "jest": "jest",
    "lint": "yarn run tslint \"src/**/*.ts\"",
    "node": "node",
    "postinstall": "yarn run webdriver:update",
    "postversion": "git push && git push --tags",
    "preclean:install": "yarn run clean",
    "preversion": "yarn test",
    "protractor": "protractor",
    "protractor:delay": "sleep 3 && yarn run protractor",
    "protractor:live": "protractor --elementExplorer",
    "rimraf": "rimraf",
    "server:dev:hmr": "yarn run server:dev --inline --hot",
    "server:dev": "yarn run webpack-dev-server --config config/webpack.dev.js --open --progress --profile --watch --content-base src/",
    "server:prod": "http-server dist -c-1 --cors",
    "server:prod:ci": "http-server dist -p 3000 -c-1 --cors",
    "server": "yarn run server:dev",
    "start:hmr": "yarn run server:dev:hmr",
    "start": "yarn run server:dev",
    "test": "yarn run lint && karma start",
    "tslint": "tslint",
    "typedoc": "typedoc",
    "version": "yarn run build",
    "watch:dev:hmr": "yarn run watch:dev --hot",
    "watch:dev": "yarn run build:dev --watch",
    "watch:prod": "yarn run build:prod --watch",
    "watch:test": "yarn run test --auto-watch --no-single-run",
    "watch": "yarn run watch:dev",
    "webdriver-manager": "webdriver-manager",
    "webdriver:start": "yarn run webdriver-manager start",
    "webdriver:update": "webdriver-manager update",
    "webpack-dev-server": "node --max_old_space_size=4096 node_modules/webpack-dev-server/bin/webpack-dev-server.js",
    "webpack": "node --max_old_space_size=4096 node_modules/webpack/bin/webpack.js",
    "commitmsg": "validate-commit-msg",
    "validate:circleci": "circleci config validate -c .circleci/config.yml",
    "cm": "./node_modules/.bin/git-cz",
    "semantic-release": "semantic-release",
    "check:next-release": "cd dist_package/ && ../node_modules/.bin/semantic-release --dry-run",
    "precommit": "lint-staged",
    "lint-staged": "lint-staged",
    "contributors:add": "./node_modules/.bin/all-contributors add",
    "contributors:generate": "./node_modules/.bin/all-contributors generate",
    "contributors:check": "./node_modules/.bin/all-contributors check",
    "//=> Section: Documentation": "",
    "docs:toc": "doctoc --title '**Table of Contents**' --maxlevel 4 ./README.md",
    "//=> Section: Library": "",
    "build:lib": "gulp build",
    "build:lib:sim": "gulp build && yarn run build:aot:prod --env.sim",
    "test:lib:ci": "yarn run jest --config jest.library.config.json --coverage",
    "test:lib": "yarn run jest --config jest.library.config.json --watch --verbose",
    "start:lib:sim": "gulp build && yarn run server:dev --env.sim",
    "lib:sync": "gulp misc:syncConfig",
    "lint:lib": "./node_modules/.bin/tslint --fix --project ./tsconfig.lint.json",
    "lint:lib:ci": "./node_modules/.bin/tslint --project ./tsconfig.lint.json",
    "codecov:upload": "./node_modules/.bin/codecov -f coverage/*.json"
  },
  "devDependencies": {
    "@angular-devkit/build-optimizer": "^0.0.32",
    "@angular/animations": "5.2.5",
    "@angular/common": "5.2.5",
    "@angular/compiler": "5.2.5",
    "@angular/compiler-cli": "5.2.5",
    "@angular/core": "5.2.5",
    "@angular/forms": "5.2.5",
