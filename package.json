    "zone.js": "^0.8.12"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/GetTerminus/ngx-tools.git"
  },
  "bugs": {
    "url": "https://github.com/GetTerminus/ngx-tools/issues"
  },
  "engines": {
    "node": ">= 8.0.0",
    "npm": ">= 5.0.0",
    "yarn": ">= 1.0.0"
  },
  "libConfig": {
    "scope": "@terminus",
    "packages": [
      "ngx-tools"
    ]
  },
  "lint-staged": {
    "src/**/ngx-tools/**/*.ts": [
      "lint:lib",
      "git add"
    ]
  },
  "config": {
    "loglevel": "verbose",
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": "./config/cz-config.js"
    }
  },
  "release": {
    "verifyConditions": [
      "condition-circle",
      "@semantic-release/npm",
      "@semantic-release/github"
    ],
    "getLastRelease": "@semantic-release/npm",
    "publish": [
      {
        "path": "@semantic-release/exec",
        "cmd": "./scripts/inject-version.sh ${nextRelease.version}"
      },
      {
        "path": "@semantic-release/npm",
        "npmPublish": true,
        "pkgRoot": "dist_package/@terminus/ngx-tools"
      },
      {
        "path": "@semantic-release/github",
        "npmPublish": false
      }
    ]
  }
}
