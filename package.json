    "convert-source-map": "^1.5.0",
    "copy-webpack-plugin": "^4.0.1",
    "core-js": "^2.4.1",
    "cross-env": "^5.0.0",
    "css-loader": "^0.28.0",
    "cz-customizable": "5.2.0",
    "deep-equal": "^1.0.1",
